[gd_scene load_steps=4 format=2]

[ext_resource path="res://GameScreen.gd" type="Script" id=1]
[ext_resource path="res://HadamardPowUp.tscn" type="PackedScene" id=2]
[ext_resource path="res://CoinGenerator.tscn" type="PackedScene" id=3]

[node name="GameScreen" type="Node2D"]
script = ExtResource( 1 )

[node name="HadamardPowUp" parent="." instance=ExtResource( 2 )]

[node name="Timers" type="Node" parent="."]
editor/display_folded = true

[node name="CoinTimer" type="Timer" parent="Timers"]

[node name="HadPowUpTimer" type="Timer" parent="Timers"]
